{
  "$schema": "vendor/infection/infection/resources/schema.json",
  "source": {
    "directories": [
      "src"
    ],
    "excludes": [
      "Testing"
    ]
  },
  "minCoveredMsi": 100,
  "minMsi": 100,
  "mutators": {
    "@default": true,
    "global-ignoreSourceCodeByRegex": [
      "Assert::.*",
      "(.+)->assert\\(.*\\);"
    ],
    "TrueValue": {
      "ignoreSourceCodeByRegex": [
        ".*match\\s*\\(true\\).*"
      ],
    }
  },
  "logs": {
    "text": "build/infection.log",
  }
}