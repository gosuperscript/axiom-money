{
    "name": "gosuperscript/schema-money",
    "type": "library",
    "licsense": "proprietary",
    "require": {
        "php": "^8.4",
        "ext-intl": "*",
        "brick/money": "^0.10.1",
        "gosuperscript/schema": "^0.1.0",
        "superscript/interval": "^1.0",
        "superscript/monetary-interval": "^0.1.2"
    },
    "require-dev": {
        "infection/infection": "^0.29.14",
        "laravel/pint": "^1.22",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0",
        "robiningelbrecht/phpunit-coverage-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Superscript\\Schema\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superscript\\Schema\\Money\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:types": "vendor/bin/phpstan analyse",
        "test:unit": "vendor/bin/phpunit -d --min-coverage=100",
        "test:infection": "vendor/bin/infection --threads=max --show-mutations",
        "test": [
            "@test:types",
            "@test:unit",
            "@test:infection"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://mannum.github.io/satis"
        }
    ]
}
